{
  "expo": {
    "name": "Projeto_final-MBL",
    "slug": "Projeto_final-MBL",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconGlimp.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/iconGlimp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconGlimp.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/iconGlimp.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-asset",
      "expo-font"
      
    ]
  }
}
